{
  "name": "matthewfaucher-book-ui",
  "version": "1.0.0",
  "description": "Pro MERN Stack Book UI",
  "main": "index.js",
  "scripts": {
    "#start": "UI server. HMR is enabled in dev mode.",
    "start": "node dist/server.js",
    "#lint": "Runs ESLint on all relevant files",
    "lint": "eslint server src browser --ext js,jsx",
    "#compile": "Generate JS bundles for production. Use with start.",
    "compile": "webpack --mode production",
    "#watch-server-hmr": "Recompile server HMR bundle on changes.",
    "watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
    "#dev-all": "Dev mode: watch for server changes and start UI server",
    "dev-all": "rm dist/* && npm run watch-server-hmr & sleep 5 && npm start",
    "heroku-postbuild": "npm rum compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ccs.neu.edu/NEU-CS5610-SU21/MatthewFaucher-Book.git"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "author": "Matt Faucher",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.4.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.21.0",
    "isomorphic-fetch": "^3.0.0",
    "nodemon": "^2.0.12",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-bootstrap": "^0.33.1",
    "react-dom": "^16.14.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.4",
    "serialize-javascript": "^6.0.0",
    "source-map-support": "^0.5.19",
    "url-search-params": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
